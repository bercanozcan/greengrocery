<h2>Meyveler</h2>
<div class="row">
    {{#each contents}}
    <div class="col-6 col-md-4 col-lg-3">
        <div class="card">
            <div class="qty">
                <button class="qty-close"><i class="fas fa-times fa-lg"></i></button>
                <button class="crease increase"><i class="fas fa-plus fa-lg"></i></button>
                <input type="text" name="qty" class="qty-input" value="0">
                <button class="crease decrease"><i class="fas fa-minus fa-lg"></i></button>
            </div>
            <div class="add-cart">
                <button class="btn-add-cart"><i class="fas fa-plus fa-lg"></i></button>
            </div>
            <img src="https://www.greenada.com/Uploads/UrunResimleri/thumb/greenadakumkuat---kamkat-5f62.jpg" class="card-img-top" alt="{{ this.title }}">
            <div class="card-body">
                <h4 class="card-title">{{ this.title }} {{ this.title }} {{ this.title }}</h4>
                <span>1 kg</span>
            </div>
        </div>
    </div>
    {{/each}}
</div>